import random 

# this is a python module 

rand_number = random.randint(1,10)

# you can also import your own modules 

random_float = random.random() #This prints a float between 0.0 and 1.0 but not including 1.0

# you can * by an int to change the range of numbers in the random float generator 


# LISTS

#This is a data structure 
# [,,]
states_of_america = ["delaware", "new york", "cali"]
#the order of the list kept 
print(states_of_america[0]) #you can also use negative index -1,-2 etc

#changing things in the list 
states_of_america[1] = "Pencilvania"

#adding to list 
states_of_america.append("UK")
print(states_of_america)
# states_of_america.extend["ireland", "india", "pakistan"]

#nested lists 
fruits = ["strawbs", "cherries", "grapes", "peaches"]
vegtables = ["spinach", "kale"]
ingredients = [fruits, vegtables]
print(ingredients[1][1])